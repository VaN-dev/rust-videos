{% extends '::layout.html.twig' %}

{% block main %}
    <div class="main">

        <div class="container">
            {{ form_start(form) }}

                {{ form_errors(form) }}

                {{ form_row(form.title) }}
                {{ form_row(form.url) }}
                {{ form_row(form.category) }}

                <button type="submit" class="btn btn-default">Submit</button>
            {{ form_end(form) }}
        </div>

    </div>
{% endblock %}